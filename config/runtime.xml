   <object-type name="Tool Link Node" parent="Tool">
      <canvas-object/>
   </object-type>
   <object-type name="LinkData"/>
   <object-type name="CanvasLinkData">
      <canvas-object/>
      <canvas-object-basic>
         <items>
            <group name="link_label">
               <text name="tag">
                  <textColor>
                     <string value="white"/>
                     <alpha value="0.75"/>
                  </textColor>
                  <outlineColor>
                     <string value="black"/>
                     <alpha value="0.75"/>
                  </outlineColor>
                  <backgroundColor>
                     <string value="black"/>
                     <alpha value="0.75"/>
                  </backgroundColor>
                  <translate x="0" y="25"/>
                  <alignment value="center"/>
                  <scale x="1.5" y="1.5"/>
               </text>
               <text name="tag-highlight">
                  <textColor>
                     <string value="yellow"/>
                     <alpha value="0.75"/>
                  </textColor>
                  <outlineColor>
                     <string value="black"/>
                     <alpha value="0.75"/>
                  </outlineColor>
                  <backgroundColor>
                     <string value="black"/>
                     <alpha value="0.75"/>
                  </backgroundColor>
                  <translate x="0" y="25"/>
                  <alignment value="center"/>
                  <scale x="1.5" y="1.5"/>
               </text>
            </group>
         </items>
         <text>
            <item name="tag" attribute="mind_label"/>
            <item name="tag-highlight" attribute="mind_label"/>
         </text>
         <switch>
            <state group="link_label">
               <item name="tag"/>
            </state>
            <state name="Highlight" group="link_label">
               <item name="tag-highlight"/>
            </state>
         </switch>
      </canvas-object-basic>
   </object-type>
